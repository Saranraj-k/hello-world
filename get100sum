import java.util.Scanner;
class hun
{
public static void main(String args[])
{
Scanner scan=new Scanner(System.in);
int n=scan.nextInt();
int a[]=new int[n];
int i ,sum=0;
for ( i=0;i<n;i++)
{
a[i]=scan.nextInt();
}
for(i=0;i<n;i++)
{
if((a[i]%2==0)&&(a[i]>=0))
{
sum=sum+a[i];
if(sum>=100)
{
System.out.println(sum);
break;
}
}
}
}
}
