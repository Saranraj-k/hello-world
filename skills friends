import java.util.Scanner;
class TestClass {
    public static void main(String args[] ) throws Exception {
        Scanner scan=new Scanner(System.in);
        int n=scan.nextInt();
        int min=scan.nextInt();
        for(int i=0;i<n;i++)
        {
            int have=scan.nextInt();
            if(have >= min)
            {
                System.out.println("YES");
            }
            else
            {
                System.out.println("NO");
            }
        }
    }
}
