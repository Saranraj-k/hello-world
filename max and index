import java.util.Scanner;
class max
{
public static void main(String args[])
{
Scanner scan=new Scanner(System.in);
int n=scan.nextInt();
int a[]=new int[n];
int i;
for ( i=0;i<n;i++)
{
a[i]=scan.nextInt();
}
int res=a[0];
for( i=1;i<n;i++)
{
if(a[i]>res){
res=a[i];
}
}
System.out.println(res);
for( i=n-1;i>0;i--)
{
if(a[i]==res)
{
System.out.println(i);
break;
}
}
}
}
