import java.util.*;
class TestClass {
    public static void main(String args[] ) throws Exception {
        
        Scanner scan= new Scanner(System.in);
        long pro=1;
        long val=1000000000 + 7;
        int n=scan.nextInt();
        int arr[]=new int[n];
       
        for(int i=0;i<n;i++)
        {
            arr[i]=scan.nextInt();
            pro=(pro*arr[i])%val;
        }
System.out.println(pro);
    }
}
