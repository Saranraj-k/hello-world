import java.util.Scanner;
class TestClass {
    public static void main(String args[] ) throws Exception {
        Scanner scan=new Scanner(System.in);
        int n=scan.nextInt();
        if(n<5)
        {
            System.out.println("1");
        }
       else
       {
           int d=n / 5;
           int f=n % 5;
           if(f != 0 )
           {
               System.out.println(d + 1);
               
           }
           else 
           {
           System.out.println(d );
           }
       }
    }
}
