import java.util.Scanner;
class TestClass {
    public static void main(String args[] ) throws Exception {
        Scanner scan=new Scanner(System.in);
        for(int i=0;i<2;i++)
        {
            System.out.println("*   *");
           
        }
        int n=scan.nextInt();
        for(int k=0;k<n;k++)
        {
            System.out.println("*****");
            for(int j=0;j<2;j++)
        {
            System.out.println("*   *");
           
        }
        }
        
    }
}
